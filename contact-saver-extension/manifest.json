{
  "manifest_version": 3,
  "name": "Contacts Extension",
  "version": "1.0",
  "description": "Manage your contacts with add, delete, and edit functionality.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Contacts"
  },
  "permissions": [
    "storage"
  ]
}
