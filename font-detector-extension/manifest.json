{
    "manifest_version": 3,
    "name": "Font Detector",
    "version": "1.0",
    "description": "Displays the font family of the highlighted text.",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  